{
  "name": "gfl",
  "version": "1.0.0",
  "description": "Gamer Fun League (GFL) is a fun Overwatch League manager tool. Teams can register, find scrims, and enter weekly games.",
  "main": "app.js",
  "scripts": {
    "prebuild": "tslint -c tslint.json -p tsconfig.json --fix",
    "build": "tsc",
    "prestart": "npm run build",
    "start": "node ./dist/app.js",
    "dev": "nodemon",
    "test": "node ./tests/postman-tests.js"
  },
  "keywords": [
    "gamer",
    "fun",
    "league",
    "GFL",
    "Overwatch",
    "OW",
    "scrim",
    "tournament"
  ],
  "author": "Michael Feist",
  "repository": {
    "type": "git",
    "url": "https://github.com/mdfeist/gfl.git"
  },
  "license": "MIT",
  "dependencies": {
    "bcrypt": "^5.0.0",
    "config": "^3.3.1",
    "dom-parser": "^0.1.6",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.9.26",
    "morgan": "^1.10.0",
    "node-fetch": "^2.6.0",
    "overwatch-stats-api": "^2.1.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/config": "0.0.36",
    "@types/dom-parser": "^0.1.0",
    "@types/express": "^4.17.7",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/mongoose": "^5.7.34",
    "@types/morgan": "^1.9.1",
    "@types/node": "^14.0.27",
    "@types/node-fetch": "^2.5.7",
    "newman": "^5.1.2",
    "nodemon": "^2.0.4",
    "ts-node": "^8.10.2",
    "tslint": "^6.1.3",
    "typescript": "^3.9.7"
  }
}
